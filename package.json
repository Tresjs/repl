    "@tresjs/core": "^3.6.1",
    "@tresjs/eslint-config-vue": "^0.2.1",
    "@types/codemirror": "^5.60.15",
    "@types/node": "^20.11.5",
    "@types/three": "^0.160.0",
    "@unocss/preset-web-fonts": "^0.58.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@volar/cdn": "~1.11.1",
