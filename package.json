    "@tresjs/cientos": "4.0.3",
    "@tresjs/core": "^4.3.1",
    "@tresjs/eslint-config-vue": "^0.2.1",
    "@types/codemirror": "^5.60.15",
    "@types/node": "^20.17.0",
    "@types/three": "^0.169.0",
    "@unocss/preset-web-fonts": "^0.63.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "@volar/cdn": "~1.11.1",
