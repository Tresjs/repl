    "monaco-textmate": "^3.0.1",
    "monaco-volar": "^0.4.0",
    "onigasm": "^2.2.5",
    "path-browserify": "^1.0.1",
    "pathe": "^1.1.2",
    "prettier": "^3.3.3",
    "release-it": "^17.10.0",
    "simple-git-hooks": "^2.11.1",
    "sucrase": "^3.35.0",
    "three": "^0.169.0",
    "typescript": "^5.6.3",
