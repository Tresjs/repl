    "monaco-textmate": "^3.0.1",
    "monaco-volar": "^0.4.0",
    "onigasm": "^2.2.5",
    "path-browserify": "^1.0.1",
    "pathe": "^1.1.2",
    "prettier": "^3.2.4",
    "release-it": "^17.0.1",
    "simple-git-hooks": "^2.9.0",
    "sucrase": "^3.35.0",
    "three": "^0.160.1",
    "typescript": "^5.3.3",
